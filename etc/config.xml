<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <channelengine>
                <active>1</active>
                <order_status>pending_payment</order_status>
                <payment_action>true</payment_action>
                <model>ChannelEngine\Magento2\Model\Payment\ChannelEngine</model>
                <group>offline</group>
            </channelengine>
        </payment>
        <carriers>
            <channelengine>
                <active>1</active>
                <sallowspecific>0</sallowspecific>
                <model>ChannelEngine\Magento2\Model\Carrier\ChannelEngine</model>
                <method_title>ChannelEngine</method_title>
                <carrier_title>ChannelEngine</carrier_title>
            </channelengine>
        </carriers>
    </default>
</config>
